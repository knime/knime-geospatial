               serializerClass="org.knime.geospatial.core.data.cell.GeoCell$CellSerializer">
         </serializer>
         <ValueFactory
               deprecated="false"
               valueFactoryClass="org.knime.geospatial.core.data.cell.GeoCell$ValueFactory">
